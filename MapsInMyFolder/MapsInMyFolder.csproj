<Project Sdk="Microsoft.NET.Sdk">
<!-- dotnet publish - -runtime win-x64 -c Release - -self-contained -->
  
    
<PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFrameworks>net6.0-windows</TargetFrameworks>
    <TargetFramework>net6.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <Platforms>x64</Platforms>
   <PlatformTarget>x64</PlatformTarget>
  <RuntimeIdentifiers>win-x64</RuntimeIdentifiers>
  <RuntimeIdentifier>win-x64</RuntimeIdentifier>
  </PropertyGroup>

  <PropertyGroup>
    <SatelliteResourceLanguages>fr</SatelliteResourceLanguages>
    <CefSharpAnyCpuSupport>true</CefSharpAnyCpuSupport>
    <CefSharpBuildAction>Content</CefSharpBuildAction>
<ProduceReferenceAssembly>True</ProduceReferenceAssembly>
    
    <Prefer32Bit>false</Prefer32Bit>
    <ErrorReport>prompt</ErrorReport>
    <SignAssembly>False</SignAssembly>
    <AssemblyOriginatorKeyFile>MapsInMyFolder.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>

  <PropertyGroup>
    <NoWarn>CS8002,NU1701</NoWarn>
    <ApplicationIcon>icone\icone_v2.ico</ApplicationIcon>
    <RepositoryUrl>https://github.com/SioGabx/MapsInMyFolder</RepositoryUrl>
    <Copyright>SioGabx</Copyright>
    <Version>0.4.0-alpha</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <DefineConstants>TRACE</DefineConstants>
    <Optimize>False</Optimize>
  </PropertyGroup>
  
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <Optimize>False</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="closedhand.cur" />
    <None Remove="download_panel.html" />
    <None Remove="editorTheme\Json.xshd" />
    <None Remove="layer_panel.html" />
    <None Remove="ScriptEditorTheme.xshd" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="icone\icone_v2.ico" />
    <EmbeddedResource Include="editorTheme\Javascript.xshd" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="cursors\closedhand.cur" />
    <EmbeddedResource Include="editorTheme\Json.xshd" />
    <EmbeddedResource Include="html\download_panel.html" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AvalonEdit" Version="6.2.0.78" />
    <PackageReference Include="cef.redist.x64" Version="107.1.12" />
    <PackageReference Include="CefSharp.Common" Version="107.1.120" />
    <PackageReference Include="CefSharp.Common.NETCore" Version="107.1.120" />
    <PackageReference Include="CefSharp.Wpf" Version="107.1.120" />
    <PackageReference Include="Jint" Version="3.0.0-beta-2043" />
    <PackageReference Include="ModernWpfUis" Version="1.4.0" />
    <PackageReference Include="NetVips" Version="2.2.0" />
    <PackageReference Include="NetVips.Native" Version="8.13.2" />
    <PackageReference Include="NetVips.Native.win-x64" Version="8.13.2" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
    <PackageReference Include="System.Data.SQLite" Version="1.0.117" />
    <PackageReference Include="wix" Version="3.11.2" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="html\layer_panel.html" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\.editorconfig" Link=".editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\MapsInMyFolder.Common\MapsInMyFolder.Commun.csproj" />
    <ProjectReference Include="..\MapsInMyFolder.MapControl\MapControl\MapsInMyFolder.MapControl.csproj" />
    <ProjectReference Include="..\MapsInMyFolder.VectorTileRenderer\MapsInMyFolder.VectorTileRenderer.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="VC_redist.x64.exe">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="chromiumembeddedframework.runtime.win-arm64" Version="104.4.24" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="chromiumembeddedframework.runtime.win-x64" Version="104.4.24" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="chromiumembeddedframework.runtime.win-x86" Version="104.4.24" />
  </ItemGroup>

