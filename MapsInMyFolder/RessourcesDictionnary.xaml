<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:avalonEdit="clr-namespace:ICSharpCode.AvalonEdit;assembly=ICSharpCode.AvalonEdit"
    xmlns:behavior="clr-namespace:BlackPearl.Controls.CoreLibrary.Behavior;assembly=BlackPearl.Controls.CoreLibrary"
    xmlns:cbcontrols="clr-namespace:BlackPearl.Controls.CoreLibrary;assembly=BlackPearl.Controls.CoreLibrary"
    xmlns:commun="clr-namespace:MapsInMyFolder.Commun;assembly=MapsInMyFolder.Commun"
    xmlns:editing="clr-namespace:ICSharpCode.AvalonEdit.Editing;assembly=ICSharpCode.AvalonEdit"
    xmlns:local="clr-namespace:MapsInMyFolder"
    xmlns:map="clr-namespace:MapsInMyFolder.MapControl;assembly=MapsInMyFolder.MapControl"
    xmlns:rendering="clr-namespace:ICSharpCode.AvalonEdit.Rendering;assembly=ICSharpCode.AvalonEdit"
    xmlns:sys="clr-namespace:System;assembly=netstandard"
    xmlns:ui="http://schemas.modernwpf.com/2019">

    <Style TargetType="avalonEdit:TextEditor">
        <Setter Property="Background" Value="#303031" />
        <Setter Property="Foreground" Value="#BCBCBC" />
        <Setter Property="LineNumbersForeground" Value="#626363" />

    </Style>

    <Style TargetType="{x:Type rendering:TextView}">

        <Setter Property="NonPrintableCharacterBrush" Value="Transparent" />
        <Setter Property="LinkTextForegroundBrush" Value="#FFD69D85" />
        <!--  Style hyperlink displayed within text in text editor  -->
        <Setter Property="LinkTextBackgroundBrush" Value="Transparent" />
    </Style>


    <Style TargetType="{x:Type editing:TextArea}">
        <Setter Property="SelectionBrush" Value="#6684470B" />
        <Setter Property="Margin" Value="0,0,0,10" />
        <Setter Property="SelectionBorder">
            <Setter.Value>
                <Pen>
                    <Pen.Brush>
                        <SolidColorBrush Color="Transparent" />
                    </Pen.Brush>
                </Pen>
            </Setter.Value>
        </Setter>
        <Setter Property="SelectionCornerRadius" Value="0" />
    </Style>

    <SolidColorBrush x:Key="DropDownButtonForegroundSecondaryPressed" Color="#BCBCBC" />

    <MenuItem
        x:Key="ContextMenu_Copier"
        x:Shared="false"
        Command="{x:Static ApplicationCommands.Copy}"
        Header="{Binding Source={x:Static commun:Languages.Current}, Path=[editorContextMenuCopy]}"
        InputGestureText="Ctrl+C">
        <MenuItem.Icon>
            <ui:FontIcon Foreground="#888989" Glyph="&#xE16F;" />
        </MenuItem.Icon>
    </MenuItem>
    <MenuItem
        x:Key="ContextMenu_Couper"
        x:Shared="false"
        Command="{x:Static ApplicationCommands.Cut}"
        Header="{Binding Source={x:Static commun:Languages.Current}, Path=[editorContextMenuCut]}"
        InputGestureText="Ctrl+X">
        <MenuItem.Icon>
            <ui:FontIcon Foreground="#888989" Glyph="&#xE16B;" />
        </MenuItem.Icon>
    </MenuItem>
    <MenuItem
        x:Key="ContextMenu_Coller"
        x:Shared="false"
        Command="{x:Static ApplicationCommands.Paste}"
        Header="{Binding Source={x:Static commun:Languages.Current}, Path=[editorContextMenuPaste]}"
        InputGestureText="Ctrl+V">
        <MenuItem.Icon>
            <ui:FontIcon Foreground="#888989" Glyph="&#xE16D;" />
        </MenuItem.Icon>
    </MenuItem>

    <ContextMenu
        x:Key="CopyPasteContextMenu"
        x:Name="CopyPasteContextMenu"
        Background="#171719"
        Foreground="#E2E2E1">
        <StaticResourceExtension ResourceKey="ContextMenu_Copier" />
        <StaticResourceExtension ResourceKey="ContextMenu_Couper" />
        <StaticResourceExtension ResourceKey="ContextMenu_Coller" />
    </ContextMenu>

    <SolidColorBrush x:Key="MultiSelectCombobox.SelectedItem.BackgroundColor" Color="#84470B" />
    <SolidColorBrush x:Key="MultiSelectCombobox.SelectedItem.ForegroundColor" Color="#BCBCBC" />

    <Style
        x:Key="MultiselectCombobox.SelectedTextBlockStyle"
        BasedOn="{StaticResource {x:Type TextBlock}}"
        TargetType="TextBlock">
        <Setter Property="Margin" Value="0,2,5,0" />
        <Setter Property="LineHeight" Value="20" />
        <Setter Property="Padding" Value="5,1,3,1" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="Background" Value="{DynamicResource MultiSelectCombobox.SelectedItem.BackgroundColor}" />
        <Setter Property="Foreground" Value="{DynamicResource MultiSelectCombobox.SelectedItem.ForegroundColor}" />
        <Style.Triggers>
            <Trigger Property="Text" Value="">
                <Setter Property="Background" Value="Transparent" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="MultiselectCombobox.RunStyle" TargetType="Run">
        <!--<Setter Property=""
                Value="0,2,5,2" />-->
        <!--<Setter Property="Padding"
                Value="7,1,7,1" />-->
        <Setter Property="BaselineAlignment" Value="Center" />
        <Setter Property="Foreground" Value="{DynamicResource MultiSelectCombobox.SelectedItem.ForegroundColor}" />
        <Setter Property="FontWeight" Value="Normal" />
    </Style>


    <Style TargetType="{x:Type cbcontrols:MultiSelectCombobox}">

        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type cbcontrols:MultiSelectCombobox}">
                    <Border
                        x:Name="border"
                        Background="{TemplateBinding Background}"
                        BorderBrush="Transparent"
                        BorderThickness="0,0,0,2"
                        SnapsToDevicePixels="True">
                        <Grid>
                            <RichTextBox
                                Name="rtxt"
                                Padding="5,5,0,2"
                                VerticalContentAlignment="Center"
                                AcceptsReturn="False"
                                BorderBrush="Transparent"
                                CaretBrush="#F18712"
                                ContextMenu="{StaticResource CopyPasteContextMenu}"
                                FontSize="{Binding FontSize, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Control}}"
                                Foreground="{DynamicResource MultiSelectCombobox.SelectedItem.ForegroundColor}"
                                IsDocumentEnabled="False">
                                <RichTextBox.Resources>
                                    <Style BasedOn="{StaticResource MultiselectCombobox.SelectedTextBlockStyle}" TargetType="TextBlock" />
                                    <Style BasedOn="{StaticResource MultiselectCombobox.RunStyle}" TargetType="Run" />
                                </RichTextBox.Resources>

                            </RichTextBox>

                            <Popup
                                Name="popup"
                                Width="{Binding ActualWidth, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=Grid}}"
                                MaxHeight="300"
                                Placement="Bottom"
                                PlacementTarget="{Binding ElementName=rtxt}"
                                PopupAnimation="Slide"
                                SnapsToDevicePixels="False"
                                StaysOpen="False">

                                <ListBox
                                    Name="lstSuggestion"
                                    HorizontalContentAlignment="Stretch"
                                    SelectionMode="Multiple">
                                    <ListBox.Resources>
                                        <Style BasedOn="{StaticResource {x:Type ListBoxItem}}" TargetType="ListBoxItem">
                                            <Setter Property="behavior:ListBoxItemBehavior.IsBroughtIntoViewWhenSelected" Value="True" />
                                            <Setter Property="Margin" Value=".5" />
                                        </Style>
                                    </ListBox.Resources>

                                </ListBox>
                            </Popup>
                        </Grid>
                    </Border>

                    <ControlTemplate.Triggers>
                        <Trigger Property="BorderBrush" Value="#5A5A5A">
                            <Setter TargetName="border" Property="BorderBrush" Value="#5A5A5A" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter TargetName="border" Property="Opacity" Value="0.56" />
                        </Trigger>

                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsKeyboardFocusWithin" Value="False" />
                                <Condition Property="IsMouseOver" Value="True" />
                            </MultiTrigger.Conditions>
                            <MultiTrigger.Setters>
                                <Setter TargetName="border" Property="BorderBrush" Value="#5A5A5A" />
                            </MultiTrigger.Setters>
                        </MultiTrigger>
                        <Trigger Property="IsKeyboardFocusWithin" Value="true">
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation
                                            FillBehavior="HoldEnd"
                                            Storyboard.TargetName="border"
                                            Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)"
                                            To="#F18712"
                                            Duration="0:0:0.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation
                                            FillBehavior="HoldEnd"
                                            Storyboard.TargetName="border"
                                            Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)"
                                            To="Transparent"
                                            Duration="0:0:0.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="TextBoxCleanStyleDefault" TargetType="{x:Type TextBox}">
        <Setter Property="Foreground" Value="#5A5A5A" />
        <Setter Property="BorderBrush" Value="#5A5A5A" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="SelectionBrush" Value="#84470B" />
        <Setter Property="Height" Value="35" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Padding" Value="5,0,0,0" />
        <Setter Property="Cursor" Value="IBeam" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBox}">
                    <Border
                        x:Name="border"
                        Background="#303031"
                        BorderBrush="Transparent"
                        BorderThickness="0,0,0,2"
                        SnapsToDevicePixels="True">
                        <ScrollViewer
                            x:Name="PART_ContentHost"
                            Focusable="false"
                            HorizontalScrollBarVisibility="Hidden"
                            VerticalScrollBarVisibility="Hidden" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="BorderBrush" Value="#5A5A5A">
                            <Setter TargetName="border" Property="BorderBrush" Value="#5A5A5A" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter TargetName="border" Property="Opacity" Value="0.56" />
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsFocused" Value="False" />
                                <Condition Property="IsMouseOver" Value="True" />
                            </MultiTrigger.Conditions>
                            <MultiTrigger.Setters>
                                <Setter TargetName="border" Property="BorderBrush" Value="#5A5A5A" />
                            </MultiTrigger.Setters>
                        </MultiTrigger>
                        <Trigger Property="IsFocused" Value="true">
                            <Setter Property="Foreground" Value="#BCBCBC" />
                            <Setter Property="CaretBrush">
                                <Setter.Value>
                                    <SolidColorBrush Color="#F18712" />
                                </Setter.Value>
                            </Setter>
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation
                                            FillBehavior="HoldEnd"
                                            Storyboard.TargetName="border"
                                            Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)"
                                            To="#F18712"
                                            Duration="0:0:0.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation
                                            FillBehavior="HoldEnd"
                                            Storyboard.TargetName="border"
                                            Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)"
                                            To="Transparent"
                                            Duration="0:0:0.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ContextMenu" Value="{StaticResource CopyPasteContextMenu}" />
    </Style>

    <Style x:Key="TextBoxCleanStyleNoBorder" TargetType="{x:Type TextBox}">
        <Setter Property="Foreground" Value="#5A5A5A" />
        <Setter Property="BorderBrush" Value="#5A5A5A" />
        <Setter Property="FontSize" Value="13" />
        <Setter Property="Foreground" Value="#BCBCBC" />
        <Setter Property="Height" Value="20" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="VerticalAlignment" Value="Top" />
        <Setter Property="SelectionBrush" Value="#84470B" />
        <Setter Property="Height" Value="35" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="Padding" Value="5,0,0,0" />
        <Setter Property="Cursor" Value="IBeam" />
        <Setter Property="CaretBrush" Value="#F18712" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBox}">
                    <Border
                        x:Name="border"
                        Background="#303031"
                        BorderBrush="Transparent"
                        BorderThickness="0,0,0,2"
                        SnapsToDevicePixels="True">
                        <ScrollViewer
                            x:Name="PART_ContentHost"
                            Focusable="false"
                            HorizontalScrollBarVisibility="Hidden"
                            VerticalScrollBarVisibility="Hidden" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ContextMenu" Value="{StaticResource CopyPasteContextMenu}" />
    </Style>


    <Style
        x:Key="TextBoxCleanStyle_16"
        BasedOn="{StaticResource TextBoxCleanStyleDefault}"
        TargetType="{x:Type TextBox}">
        <Setter Property="FontSize" Value="16" />
    </Style>
    <Style
        x:Key="TextBoxCleanStyle_13"
        BasedOn="{StaticResource TextBoxCleanStyleDefault}"
        TargetType="{x:Type TextBox}">
        <Setter Property="FontSize" Value="13" />
        <Setter Property="Foreground" Value="#BCBCBC" />
        <Setter Property="Height" Value="20" />
    </Style>


    <Style
        x:Key="TextBoxFormeStyle"
        BasedOn="{StaticResource TextBoxCleanStyle_13}"
        TargetType="TextBox">
        <Setter Property="OverridesDefaultStyle" Value="False" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="CaretBrush" Value="#F18712" />
        <Setter Property="Padding" Value="0,3,3,3" />
        <Setter Property="Height" Value="25" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="SpellCheck.IsEnabled" Value="False" />
        <Setter Property="ContextMenu" Value="{StaticResource CopyPasteContextMenu}" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type TextBox}">
                    <Border
                        x:Name="border"
                        Background="#303031"
                        BorderBrush="#5A5A5A"
                        BorderThickness="0,0,0,2"
                        SnapsToDevicePixels="True">
                        <ScrollViewer
                            x:Name="PART_ContentHost"
                            Focusable="false"
                            HorizontalScrollBarVisibility="Hidden"
                            VerticalScrollBarVisibility="Hidden" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsFocused" Value="False" />
                                <Condition Property="IsMouseOver" Value="True" />
                            </MultiTrigger.Conditions>
                            <MultiTrigger.Setters>
                                <Setter TargetName="border" Property="BorderBrush" Value="#FF646464" />
                            </MultiTrigger.Setters>
                        </MultiTrigger>
                        <Trigger Property="IsFocused" Value="true">
                            <Setter Property="Foreground" Value="#BCBCBC" />
                            <Trigger.EnterActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation
                                            FillBehavior="HoldEnd"
                                            Storyboard.TargetName="border"
                                            Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)"
                                            To="#F18712"
                                            Duration="0:0:0.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <ColorAnimation
                                            FillBehavior="HoldEnd"
                                            Storyboard.TargetName="border"
                                            Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)"
                                            To="#5A5A5A"
                                            Duration="0:0:0.2" />
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="{x:Type ListBoxItem}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ListBoxItem}">
                    <Border
                        x:Name="border_list"
                        Padding="8"
                        Background="Transparent"
                        BorderBrush="Transparent"
                        BorderThickness="0"
                        SnapsToDevicePixels="true">
                        <ContentPresenter
                            HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                            VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                            SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="Selector.IsSelectionActive" Value="True" />
                                <Condition Property="IsSelected" Value="True" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="border_list" Property="BorderBrush" Value="#F18712" />
                        </MultiTrigger>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="border_list" Property="Background" Value="#5A5A5A" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

