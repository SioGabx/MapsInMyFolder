<Application x:Class="MapsInMyFolder.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MapsInMyFolder"
             xmlns:ui="http://schemas.modernwpf.com/2019"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ui:ThemeResources RequestedTheme="Dark" AccentColor="#84470B">
                    <ui:ThemeResources.MergedDictionaries>
                        <ui:ColorPaletteResources Accent="#84470B" AltLow="#333333" AltHigh="#84470B" />
                        <ResourceDictionary Source="RessourcesDictionnary.xaml"/>
                    </ui:ThemeResources.MergedDictionaries>
                </ui:ThemeResources>
                <ui:XamlControlsResources />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
